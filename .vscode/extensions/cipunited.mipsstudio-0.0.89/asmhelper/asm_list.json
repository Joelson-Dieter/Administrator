{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "list",
	"patterns": [
		{
			"include": "#listfile"
		},
		{
			"include": "source.asm"
		}
	],
	"repository": {
		"listfile": {
			"patterns": [
				{
					"name": "constant.numeric.integer.hexadecimal.asm",
					"match": "\\b[0-9a-fA-F]{4,}(\\.\\w+)?\\s+([0-9a-fA-F]{2,}\\s)*"
				}
			]
		}
	},
	"scopeName": "source.asm.list"
}
